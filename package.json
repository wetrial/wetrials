{
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap.js",
    "build": "father-build && webpack --progress",
    "deploy": "npm run site && gh-pages -d ./dist",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./packages && npm run lint:prettier",
    "lint:prettier": "prettier --check \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "release": "node ./scripts/release.js",
    "site": "dumi build",
    "size": "npm run build && size-limit",
    "start": "dumi dev",
    "sync:tnpm": "node -e 'require(\"./scripts/syncTNPM\")()'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:update": "umi-test --updateSnapshot",
    "update:deps": "yarn upgrade-interactive --latest",
    "tsc": "lerna run tsc --stream",
    "clean": "node ./scripts/clearBuild"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@jsdevtools/npm-publish": "^1.2.0",
    "@size-limit/preset-big-lib": "^4.5.5",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.10.3",
    "@types/history": "^4.7.7",
    "@types/jest": "^26.0.0",
    "@types/memoize-one": "^5.1.2",
    "@types/node": "^14.0.26",
    "@types/qs": "^6.9.4",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-intl": "^3.0.0",
    "@types/react-responsive": "^8.0.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/warning": "^3.0.0",
    "@umijs/babel-preset-umi": "^3.2.28",
    "@umijs/fabric": "^2.4.9",
    "@umijs/plugin-analytics": "^0.2.2",
    "@umijs/test": "^3.2.28",
    "@umijs/test-utils": "^3.2.28",
    "@umijs/utils": "^3.3.3",
    "antd": "^4.10.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.12.2",
    "body-parser": "^1.18.2",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.0",
    "css-loader": "^4.2.2",
    "del": "^5.1.0",
    "dumi": "^1.1.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^7.2.0",
    "father-build": "^1.18.1",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.6",
    "jest": "^26.2.2",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^3.22.1",
    "less-loader": "^7.0.1",
    "lint-staged": "^10.0.2",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.11.0",
    "mockdate": "^3.0.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "np": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pify": "^5.0.0",
    "prettier": "^2.0.1",
    "pretty-quick": "^3.0.0",
    "query-string": "^6.11.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^4.3.1",
    "react-test-renderer": "^16.13.1",
    "size-limit": "^4.6.0",
    "slash2": "^2.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.2",
    "terser-webpack-plugin": "^4.1.0",
    "typescript": "^4.0.2",
    "umi": "^3.2.10",
    "umi-plugin-githubpages": "^2.0.1",
    "umi-test": "^1.9.6",
    "umi-types": "^0.5.14",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "write-pkg": "^4.0.0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "pretty-quick --staged",
    "commit-msg": "node scripts/verifyCommit.js"
  }
}
